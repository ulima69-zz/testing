PROC SQL;

   CREATE TABLE CBR_QUALIFICACAO_GERAL AS 
      SELECT DISTINCT 

          t1.PREFDEP, 
          t1.CTRA,
		  COUNT(t1.MCI) AS TOTAL        
		            
